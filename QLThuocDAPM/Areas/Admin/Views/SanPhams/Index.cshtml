@model IEnumerable<QLThuocDAPM.Data.SanPham>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TenSp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ThanhPhan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GiaTien)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DonVi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HansuDung)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ChitietSp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SoLuong)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SoLuongMua)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HinhAnh1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HinhAnh2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HinhAnh3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HinhAnh4)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaBenhNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaDmNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaGiamGiaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaNhaCungCapNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TenSp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ThanhPhan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GiaTien)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DonVi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HansuDung)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChitietSp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SoLuong)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SoLuongMua)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HinhAnh1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HinhAnh2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HinhAnh3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HinhAnh4)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaBenhNavigation.MaBenh)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaDmNavigation.MaDm)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaGiamGiaNavigation.MaGiamGia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaNhaCungCapNavigation.MaNhaCungCap)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaSp">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaSp">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaSp">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
